{
  "name": "webmontag-react-shoppingchart",
  "version": "0.0.0",
  "description": "A shopping chard example in react",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "build:before": "mkdir -p dist && mkdir -p dist/js",
    "build:js": "browserify src/js/index.js -s app --outfile=dist/js/index.js -t [babelify --optional es7.classProperties]",
    "build:copy": "cp src/index.html dist/index.html",
    "build": "npm run build:before && npm run build:js && npm run build:copy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ranzwertig/webmontag-react-shoppingchart.git"
  },
  "author": "Christian Ranz <cr@codastic.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ranzwertig/webmontag-react-shoppingchart/issues"
  },
  "homepage": "https://github.com/ranzwertig/webmontag-react-shoppingchart",
  "devDependencies": {
    "babel-core": "^5.6.15",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "envify": "^3.4.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4"
  },
  "dependencies": {
    "flux": "^2.0.3",
    "immutable": "^3.7.4",
    "keymirror": "^0.1.1",
    "react": "^0.13.3"
  }
